####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: wavfft.pro
	tmake wavfft.pro -o Makefile

dist:
	$(TAR) wavfft.tar wavfft.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) wavfft.tar

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(UICIMPLS) $(UICDECLS) $(TARGET)
	-rm -f *~ core

